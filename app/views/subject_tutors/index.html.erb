<h1>Listing subject_tutors</h1>

<table>
  <thead>
    <tr>
      <th>Subjectid</th>
      <th>Userid</th>
      <th>Rateperhour</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @subject_tutors.each do |subject_tutor| %>
      <tr>
        <td><%= subject_tutor.subjectID %></td>
        <td><%= subject_tutor.userID %></td>
        <td><%= subject_tutor.rateperhour %></td>
        <td><%= link_to 'Show', subject_tutor %></td>
        <td><%= link_to 'Edit', edit_subject_tutor_path(subject_tutor) %></td>
        <td><%= link_to 'Destroy', subject_tutor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

